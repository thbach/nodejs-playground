{
  "extends": ["airbnb-base", "prettier"],
  "parser": "babel-eslint",
  "rules": {
    "no-underscore-dangle": 1,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "semi": true
      }
    ]
  },
  "plugins": ["prettier"]
}
